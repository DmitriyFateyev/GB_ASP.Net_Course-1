@model IEnumerable<WebStore.Models.Employee>

@{ 
    ViewData["Title"] = "Employees List";
}

<table class="table">
    <tr>
        <th>id</th>
        <th>Lastname</th>
        <th>Firstname</th>
        <th>Patronymic</th>
        <th>Age</th>
        <th>Details</th>
        <th>e-mail</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.id</th>
            <th>@item.Lastname</th>
            <th>@item.Name</th>
            <th>@item.Patronymic</th>
            <th>@item.Age</th>
            <th>@item.email</th>
            <th><a asp-controller="Employees" asp-action="Details" asp-route-id="@item.id">Details</a></th>
        </tr>
    }
</table>
<hr />
<a asp-controller="Home" asp-action="Index">Home</a>