@model WebStore.Models.Employee
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <h1>id:@Model.id employee details page</h1>
    <hr />

    <table border="1">
        <thead>
            <tr>
                <th>id</th>
                <th>Lastname</th>
                <th>Firstname</th>
                <th>Patronymic</th>
                <th>Age</th>
            </tr>
            <tr>
                <th>@Model.id</th>
                <th>@Model.Lastname</th>
                <th>@Model.Name</th>
                <th>@Model.Patronymic</th>
                <th>@Model.Age</th>
            </tr>
        </thead>
    </table>
    <hr />
    <a href="~/Employees/EmployeesList">Back</a>
</body>
</html>
