@model WebStore.Models.Employee

@{ 
    ViewData["Title"] = "Details";
}

<div class="container">
    <table class="table">
        <tr>
            <th>id</th>
            <th>Lastname</th>
            <th>Firstname</th>
            <th>Patronymic</th>
            <th>Age</th>
            <th>e-mail</th>
        </tr>
        <tr>
            <th>@Model.id</th>
            <th>@Model.Lastname</th>
            <th>@Model.Name</th>
            <th>@Model.Patronymic</th>
            <th>@Model.Age</th>
            <th>@Model.email</th>
        </tr>
    </table>
    <hr />
    <a asp-controller="Employees" asp-action="EmployeesList">Back</a>
</div>